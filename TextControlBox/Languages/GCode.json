{
  "Highlights": [
    {
      //Axis Y
      "Pattern": "\\bY(?=([0-9]|(\\+|\\-)[0-9]))",
      "ColorDark": "#00ff00",
      "ColorLight": "#00ff00"
    },
    {
      //Axis X
      "Pattern": "\\bX(?=([0-9]|(\\+|\\-)[0-9]))",
      "ColorDark": "#ff0000",
      "ColorLight": "#ff0000"
    },
    {
      //Axis Z
      "Pattern": "\\bZ(?=([0-9]|(\\+|\\-)[0-9]))",
      "ColorDark": "#0077ff",
      "ColorLight": "#0077ff"
    },
    {
      //Axis A
      "Pattern": "\\bA(?=([0-9]|(\\+|\\-)[0-9]))",
      "ColorDark": "#ff00ff",
      "ColorLight": "#ff00ff"
    },
    {
      //Feedrate E | F
      "Pattern": "\\b(E|F)(?=([0-9]|(\\+|\\-)[0-9]))",
      "ColorDark": "#ffAA00",
      "ColorLight": "#ffAA00"
    },
    {
      //Extruder
      "Pattern": "\\b(S|T)(?=([0-9]|(\\+|\\-)[0-9]))",
      "ColorDark": "#ffff00",
      "ColorLight": "#ffff00"
    },
    {
      //-/+ floating numbers
      "Pattern": "([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?",
      "ColorDark": "#9f009f",
      "ColorLight": "#ff00ff"
    },
    {
      //G0 - G999 & M0 - M999
      "Pattern": "[G|M][0-999].*?[\\s|\\n]",
      "ColorDark": "#00ffff",
      "ColorLight": "#00aaaa"
    },
    {
      //Comments
      "Pattern": "(;|\/\/|\brem\b).*",
      "ColorDark": "#888888",
      "ColorLight": "#888888"
    }
  ],
  "Name": "GCode",
  "Filter": ".ngc|.tap|.gcode|.nc|.cnc",
  "Description": "Syntaxhighlighting for GCode language",
  "Author": "Julius Kirsch"
}