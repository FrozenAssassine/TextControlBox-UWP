{
  "Highlights": [
    {
      "Description": "Double points",
      "Pattern": "\\:",
      "ColorDark": "#FFFF00",
      "ColorLight": "#FFFF00"
    },
    {
      "Description": "Byte count",
      "Pattern": "\\:([0-9A-Fa-f]{2})",
      "ColorDark": "#00FF00",
      "ColorLight": "#00FF00"
    },
    {
      "Description": "Address",
      "Pattern": "\\:[0-9A-Fa-f]{2}([0-9A-Fa-f]{4})",
      "ColorDark": "#00FF00",
      "ColorLight": "#00FF00"
    },
    {
      "Description": "Record type",
      "Pattern": "\\:[0-9A-Fa-f]{6}([0-9A-Fa-f]{2})",
      "ColorDark": "#FF5500",
      "ColorLight": "#FF5500"
    },
    {
      "Description": "Data",
      "Pattern": "\\:[0-9A-Fa-f]{8}([0-9A-Fa-f]*)[0-9A-Fa-f]{2}",
      "ColorDark": "#00FFFF",
      "ColorLight": "#00FFFF"
    },
    {
      "Description": "Checksum",
      "Pattern": "\\:[0-9A-Fa-f]{8}[0-9A-Fa-f]*([0-9A-Fa-f]{2})",
      "ColorDark": "#666666",
      "ColorLight": "#666666"
    },
    {
      "Description": "Comment",
      "Pattern": "//.*",
      "ColorDark": "#666666",
      "ColorLight": "#666666"
    },
    {
      "Description": "Exceptions",
      "Pattern": "[^0-9A-Fa-f\\:\\n]",
      "CodeStyle": {
        "Underlined": true
      },
      "ColorDark": "#FF0000",
      "ColorLight": "#FF0000"
    }
  ],
  "Name": "HexFile",
  "Filter": "hex|bin",
  "Description": "Syntaxhighlighting for hex and binary code.",
  "Author": "Finn Freitag"
}