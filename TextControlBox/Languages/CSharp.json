{
  "Highlights": [
    {
      //-/+ floating numbers
      "Pattern": "\\b([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?\\b",
      "ColorDark": "#ff00ff",
      "ColorLight": "#ff00ff"
    },
    {
      //Functions
      "Pattern": "(?<!(def\\s))(?<=^|\\s|.)[a-zA-Z_][\\w_]*(?=\\()",
      "ColorDark": "#ffbb00",
      "ColorLight": "#880088"
    },
    {
      //Keywords
      "Pattern": "\\b(abstract|as|base|bool|break|byte|case|catch|char|checked|class|const|continue|decimal|default|delegate|do|double|else|enum|event|explicit|extern|external|false|final|finally|fixed|float|for|foreach|get|goto|if|implicit|in|int|interface|internal|is|lock|long|namespace|new|null|object|operator|out|override|params|partial|private|protected|public|readonly|ref|return|sbyte|sealed|set|short|sizeof|stackalloc|static|string|struct|switch|this|throw|true|try|typeof|uint|ulong|unchecked|unsafe|ushort|using|value|var|virtual|void|volatile|while)\\b",
      "ColorDark": "#00ffff",
      "ColorLight": "#0066bb"
    },
    {
      //Classes
      "Pattern": "\\b(List|Color|Console|Debug|Dictionary|Stack|Queue|GC)\\b",
      "ColorDark": "#ff9900",
      "ColorLight": "#008000"
    },
    {
      //try/catch/finally
      "Pattern": "\\b(try|catch|finally)\\b",
      "ColorDark": "#6666ff",
      "ColorLight": "#9922ff"
    },
    {
      //regions
      "CodeStyle": {
        "Bold": true
      },
      "Pattern": "(#region)+(.*?)($|\n)",
      "ColorDark": "#ff0000",
      "ColorLight": "#ff0000"
    },
    {
      //endregions
      "CodeStyle": {
        "Bold": true
      },
      "Pattern": "#endregion",
      "ColorDark": "#ff0000",
      "ColorLight": "#ff0000"
    },
    {
      //strings: ""
      "Pattern": "\"[^\\n]*?\"",
      "ColorDark": "#00FF00",
      "ColorLight": "#ff5500"
    },
    {
      //strings: ''
      "Pattern": "'[^\\n]*?'",
      "ColorDark": "#00FF00",
      "ColorLight": "#00CA00"
    },
    {
      //Comment multiline /**/
      "Pattern": "/\\*[^*]*\\*+(?:[^/*][^*]*\\*+)*/",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    },
    {
      //Comment singleline //
      "Pattern": "//(.*?)\\r?\\n",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    }
  ],
  "Name": "CSharp",
  "Filter": ".cs",
  "Description": "Syntaxhighlighting for CSharp language",
  "Author": "Julius Kirsch"
}