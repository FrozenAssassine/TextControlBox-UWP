{
  "Highlights": [
    {
      //-/+ floating numbers
      "Pattern": "\\b([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?\\b",
      "ColorDark": "#ff00ff",
      "ColorLight": "#ff00ff"
    },
    {
      //Functions
      "Pattern": "(?<!(def\\s))(?<=^|\\s|.)[a-zA-Z_][\\w_]*(?=\\()",
      "ColorDark": "#ffbb00",
      "ColorLight": "#880088"
    },
    {
      //Classes
      "Pattern": "\\b(System.out|System|Math)\\b",
      "ColorDark": "#ff9900",
      "ColorLight": "#008000"
    },
    {
      //Keywords
      "Pattern": "\\b(abstract|assert|boolean|break|byte|case|catch|char|class|const|continue|default|do|double|else|enum|extends|final|finally|float|for|goto|if|implements|import|instanceof|int|interface|long|native|new|package|private|protected|public|return|short|static|super|switch|synchronized|this|throw|throws|transient|try|void|volatile|while|exports|modle|non-sealed|open|opens|permits|provides|record|requires|sealed|to|transitive|uses|var|with|yield|true|false|null)\\b",
      "ColorDark": "#00ffff",
      "ColorLight": "#0066bb"
    },
    {
      //strings: ""
      "Pattern": "\"[^\\n]*?\"",
      "ColorDark": "#00FF00",
      "ColorLight": "#ff5500"
    },
    {
      //strings: ''
      "Pattern": "'[^\\n]*?'",
      "ColorDark": "#00FF00",
      "ColorLight": "#00CA00"
    },
    {
      //Comment multiline /**/
      "Pattern": "/\\*[^*]*\\*+(?:[^/*][^*]*\\*+)*/",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    },
    {
      //Comment singleline //
      "Pattern": "//(.*?)\\r?\\n",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    }
  ],
  "Name": "Java",
  "Description": "Syntaxhighlighting for Java language",
  "Author": "Julius Kirsch"
}