{
  "Highlights": [
    {
      "Description": "punctuation",
      "Pattern": "\\W",
      "ColorDark": "#CC0066",
      "ColorLight": "#990033"
    },
    {
      "Description": "operators",
      "Pattern": "(\\+|\\-|\\*|/|%|\\=|\\:|\\!|>|\\<|\\?|&|\\||\\~|\\^)",
      "ColorDark": "#77FF77",
      "ColorLight": "#77FF77"
    },
    {
      "Description": "-/+ floating numbers",
      "Pattern": "\\b([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?\\b",
      "ColorDark": "#ff00ff",
      "ColorLight": "#ff00ff"
    },
    {
      "Description": "functions",
      "Pattern": "(?<!(def\\s))(?<=^|\\s|.)[a-zA-Z_][\\w_]*(?=\\()",
      "ColorDark": "#ffbb00",
      "ColorLight": "#880088"
    },
    {
      "Description": "keywords",
      "Pattern": "\\b(goto|in|instanceof|static|arguments|public|do|else|const|function|class|return|let|eval|for|if|this|break|debugger|yield|extends|enum|continue|export|null|switch|private|new|throw|while|case|await|delete|super|default|void|var|protected|package|interface|false|typeof|implements|with|import|true)\\b",
      "ColorDark": "#00ffff",
      "ColorLight": "#0066bb"
    },
    {
      "Description": "something else",
      "Pattern": "\\b(document|window|screen)\\b",
      "ColorDark": "#33BB00",
      "ColorLight": "#008000"
    },
    {
      "Description": "try/catch/finally",
      "Pattern": "\\b(try|catch|finally)\\b",
      "ColorDark": "#6666ff",
      "ColorLight": "#9922ff"
    },
    {
      "Description": "regex patterns",
      "Pattern": "/[^\\n]*/i{0,1}",
      "ColorDark": "#FFFF00",
      "ColorLight": "#FFFF00"
    },
    {
      "Description": "strings",
      "Pattern": "[\"'][^\\n]*?[\"']",
      "ColorDark": "#00FF00",
      "ColorLight": "#ff5500"
    },
    {
      "Description": "multiline comments",
      "Pattern": "/\\*[^*]*\\*+(?:[^/*][^*]*\\*+)*/",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    },
    {
      "Description": "single line comment",
      "Pattern": "//(.*?)\\r?\\n",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    }
  ],
  "Name": "Javascript",
  "Filter": "js",
  "Description": "Syntaxhighlighting for Javascript language.",
  "Author": "Finn Freitag"
}