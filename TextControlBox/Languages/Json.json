{
  "Highlights": [
    {
      //-/+ floating numbers
      "Pattern": "\\b([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?\\b",
      "ColorDark": "#ff00ff",
      "ColorLight": "#dd00dd"
    },
    {
      //null, true, false
      "Pattern": "(null|true|false)",
      "ColorDark": "#0099ff",
      "ColorLight": "#00AADD"
    },
    {
      //Brackets
      "Pattern": "(,|{|}|\\[|\\])",
      "ColorDark": "#646464",
      "ColorLight": "#969696"
    },
    {
      "Pattern": "(\".+\")\\:",
      "ColorDark": "#dddd00",
      "ColorLight": "#00CA00"
    },
    {
      //Comment multiline /**/
      "Pattern": "/\\*[^*]*\\*+(?:[^/*][^*]*\\*+)*/",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    },
    {
      //Comment singleline //
      "Pattern": "//(.*?)\\r?\\n",
      "ColorDark": "#646464",
      "ColorLight": "#888888"
    },
    {
      //strings: ''
      "Pattern": "'[^\\n]*?'",
      "ColorDark": "#00FF00",
      "ColorLight": "#00CA00"
    },
    {
      //strings: ""
      "Pattern": "\"[^\\n]*?\"",
      "ColorDark": "#00FF00",
      "ColorLight": "#00CA00"
    }
  ],
  "Name": "Json",
  "Description": "Syntax highlighting for Json language",
  "Filter":  "json",
  "Author": "Julius Kirsch"
}